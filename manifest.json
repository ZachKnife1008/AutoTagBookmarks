{
  "manifest_version": 2,
  "name": "Auto Tag Bookmarks",
  "version": "1.0",
  "permissions": ["bookmarks", "activeTab", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  }
}
